<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="MainGradient" StartPoint="0,1" EndPoint="1,2">
        <GradientStop Offset="0" Color="#d762f5" />
        <GradientStop Offset="0.1999" Color="#d762f5" />
        <GradientStop Offset="0.2" Color="#e390f5" />
        <GradientStop Offset="0.2999" Color="#e390f5" />
        <GradientStop Offset="0.3" Color="#fc9ef6" />
        <GradientStop Offset="1" Color="#fc9ef6" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ColorContent" Color="White" />

    <SolidColorBrush x:Key="LightBackground" Color="#fc9ef6" />
    <SolidColorBrush x:Key="HardBackground" Color="#d762f5" />

    <SolidColorBrush x:Key="Card" Color="#d762f5" />
    <SolidColorBrush x:Key="Button" Color="#d762f5" />

    <SolidColorBrush x:Key="AuthenticationColor" Color="#fc9ef6" />
</ResourceDictionary>